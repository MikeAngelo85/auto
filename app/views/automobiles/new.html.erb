<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new automobile</h1>
  <%= simple_form_for @automobile, url: automobile_path do |f| %>
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :year %>
    <%= f.input :style %>
    <%= f.input :transmission %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>
